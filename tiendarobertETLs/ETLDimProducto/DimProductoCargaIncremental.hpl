    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>
SELECT
  /* última fecha ya cargada */
  (SELECT MAX(fechaInventarioKey) FROM factInventario) AS ultima_fecha_inventario,


 /* Días después de la última fecha cargada */
  (SELECT
    CAST(
      DATE_FORMAT(
        DATE_ADD(STR_TO_DATE(MAX(fechaInventarioKey), '%Y%m%d'), INTERVAL 1 DAY),
        '%Y%m%d'
      ) AS UNSIGNED
    )
  FROM factInventario
) AS fecha_actual;


/* Fecha del día actual 
  (SELECT MAX(tiempoKey)
  FROM dimtiempo
  WHERE fecha &lt;= CURDATE()) AS fecha_actual; */

</sql>
