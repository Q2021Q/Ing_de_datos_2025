    <limit>0</limit>
    <lookup>LeerFechaCorte</lookup>
    <sql>SELECT
    p.id AS productoID,
    p.nombreProducto,
    c.categoria AS categoriaProducto,
    f.familia AS familiaProducto,
    IFNULL(p.fraccionario, 'No') AS fraccionario,
    um.simbolo AS valorUnidadMedida,
    um.unidad AS unidadMedida,
    'No especificado' AS presentacion
FROM
    productos p
JOIN
    categorias c ON p.categoria = c.id
JOIN
    familias f ON p.familia = f.id
JOIN
    unidad_medidas um ON p.medidamh = um.id
WHERE
    DATE(p.created_at) > ?       -- Ultima Fecha en Inventario
AND DATE(p.created_at) &lt;= ?     -- Fecha Actual</sql>
    <variables_active>Y</variables_active>
