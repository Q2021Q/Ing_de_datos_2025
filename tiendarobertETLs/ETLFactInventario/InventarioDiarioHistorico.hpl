    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// Flujo debe venir ordenado por producto_id, sucursal_id, fechaMovimiento (ASC)

// Inicializa variables globales solo una vez (persisten entre filas)
if (typeof __lastKey === 'undefined') {
  __lastKey = null;
  __accCantidad = 0;
}

// Clave actual: combina producto y sucursal
var key = producto_id + '|' + sucursal_id;

// Si cambia de producto o sucursal, reinicia acumulador
if (key !== __lastKey) {
  __accCantidad = 0;
  __lastKey = key;
}

// Convierte a número y suma al acumulado
var qty = (stock_movimiento_diario == null) ? 0 : Number(stock_movimiento_diario);
__accCantidad += qty;

// Asigna el acumulado al campo de salida definido en “Fields”
stock_diario = __accCantidad;
</jsScript_script>
