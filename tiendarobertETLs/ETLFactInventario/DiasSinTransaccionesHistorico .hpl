  <transform>
    <name>RellenarFaltantes</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <optimizationLevel>9</optimizationLevel>
    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// ===== RellenarFaltantes (con costos unitarios de entrada) =====

// Identificador de grupo producto–sucursal
var grupo = productoKey + '|' + sucursalKey;

// Normaliza unitarios (si vienen null)
var unitSin = (costoSinIva_uni == null ? 0.0 : costoSinIva_uni * 1.0);
var unitCon = (costoConIva_uni == null ? 0.0 : costoConIva_uni * 1.0);

// Reinicio al cambiar de grupo
if (typeof __grupo === 'undefined' || __grupo != grupo) {
  __grupo = grupo;
  __ultimoStock = (stockBase == null ? 0.0 : stockBase * 1.0);
}

// Si el día ya tiene snapshot (faltante = 0), respétalo
if (faltante == 0) {
  out_stockInicial   = (stockInicial == null ? __ultimoStock : stockInicial * 1.0);
  out_stockFinal     = (stockFinal   == null ? __ultimoStock : stockFinal   * 1.0);
  out_variacionStock = out_stockFinal - out_stockInicial;

  // Costos del día = unitario * stockFinal del día
  out_costoSinIva = unitSin * out_stockFinal;
  out_costoConIva = unitCon * out_stockFinal;

  __ultimoStock = out_stockFinal; // preparar siguiente día
  nuevo = 0; // no insertar (ya existía)
} else {
  // Día faltante: copiar stock anterior
  out_stockInicial   = __ultimoStock;
  out_stockFinal     = __ultimoStock;
  out_variacionStock = 0.0;

  // Costos del día = unitario * stockFinal del día (arrastre)
  out_costoSinIva = unitSin * out_stockFinal;
  out_costoConIva = unitCon * out_stockFinal;

  // __ultimoStock no cambia (sin movimiento)
