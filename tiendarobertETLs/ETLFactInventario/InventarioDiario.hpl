    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// Requiere el flujo ORDENADO por productoKey, sucursalKey, fechaInventarioKey
var grp = productoKey_pc + "_" + sucursalKey_pc;

// Estado persistente por instancia del paso
if (typeof __grp__ === "undefined") { __grp__ = null; __prevFinal__ = 0; __inited__ = false; }

// Cambio de grupo -> reiniciar
if (grp !== __grp__) {
  __grp__ = grp;
  __prevFinal__ = 0;
  __inited__ = false;
}

// Defaults
var base = (stockBase == null ? 0 : stockBase);
var neto = (netoDia   == null ? 0 : netoDia);

// Primer día que procesamos del grupo: arranca en base
var si = (__inited__ ? __prevFinal__ : base);
var sf = si + neto;

// Salidas
stockInicial   = si;
stockFinal     = sf;
variacionStock = neto;

// Guardar estado para la próxima fila del mismo grupo
__prevFinal__ = sf;
__inited__ = true;
</jsScript_script>
      </jsScript>
    </jsScripts>
