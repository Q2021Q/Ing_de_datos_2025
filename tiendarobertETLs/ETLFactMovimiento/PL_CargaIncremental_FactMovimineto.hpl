  <transform>
    <name>Get ultima fecha del DWH </name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>DWH_MYSQL</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT
  -- 칰ltima fecha cargada en formato YYYY-MM-DD
  DATE_FORMAT(
    DATE(
      COALESCE(
        STR_TO_DATE(CAST(MAX(fm.fechaMovimientoKey) AS CHAR), '%Y%m%d'),
        STR_TO_DATE(CAST(MAX(fm.fechaMovimientoKey) AS CHAR), '%Y-%m-%d %H:%i:%s'),
        MAX(fm.fechaMovimientoKey)
      )
    ), '%Y-%m-%d'
  ) AS ultima_fecha,

  -- d칤a siguiente a la 칰ltima fecha cargada (YYYY-MM-DD)
  DATE_FORMAT(
    DATE_ADD(
      DATE(
        COALESCE(
          STR_TO_DATE(CAST(MAX(fm.fechaMovimientoKey) AS CHAR), '%Y%m%d'),
          STR_TO_DATE(CAST(MAX(fm.fechaMovimientoKey) AS CHAR), '%Y-%m-%d %H:%i:%s'),
          MAX(fm.fechaMovimientoKey)
        )
      ),
      INTERVAL 1 DAY
    ),
    '%Y-%m-%d'
  ) AS fecha_siguiente,

  -- fecha_actual: la fecha en dimTiempo m치s cercana a hoy (YYYY-MM-DD)
  DATE_FORMAT(
    (SELECT MAX(dt.fecha) FROM dimTiempo dt WHERE dt.fecha &lt;= CURDATE()),
    '%Y-%m-%d'
  ) AS fecha_actual
FROM factMovimiento fm;
</sql>
