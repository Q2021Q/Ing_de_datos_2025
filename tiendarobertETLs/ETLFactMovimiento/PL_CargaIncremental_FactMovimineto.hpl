    <connection>SRC_MYSQL</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>-- 游릴 VENTAS
SELECT 
    v.id AS idMovimiento,
    vd.producto AS productoKey,
    v.sucursal AS sucursalKey,
    1 AS tipoMovimientoKey,             -- 1 = Venta
    DATE(v.fecha) AS fechaMovimiento,
    vd.cantidad * -1 AS cantidad,       -- negativo: salida de stock
    vd.costo AS costoSinIva,
    vd.total AS costoConIva
FROM ventas v
JOIN ventas_detalles vd ON v.id = vd.venta
WHERE DATE(v.fecha) BETWEEN STR_TO_DATE('${FECHA_SIGUIENTE}', '%Y-%m-%d')
                        AND STR_TO_DATE('${FECHA_ACTUAL}', '%Y-%m-%d')

UNION ALL

-- 游릳 COMPRAS
SELECT 
    c.id AS idMovimiento,
    cd.producto AS productoKey,
    c.sucursal AS sucursalKey,
    2 AS tipoMovimientoKey,             -- 2 = Compra
    DATE(c.fecha) AS fechaMovimiento,
    cd.cantidad AS cantidad,            -- positivo: entrada de stock
    cd.costo AS costoSinIva,
    cd.total AS costoConIva
FROM compras c
JOIN compras_detalles cd ON c.id = cd.compra
WHERE DATE(c.fecha) BETWEEN STR_TO_DATE('${FECHA_SIGUIENTE}', '%Y-%m-%d')
                        AND STR_TO_DATE('${FECHA_ACTUAL}', '%Y-%m-%d')

UNION ALL

-- 游릱 DEVOLUCIONES
SELECT 
    d.id AS idMovimiento,
    dd.producto AS productoKey,
    d.sucursal AS sucursalKey,
    3 AS tipoMovimientoKey,             -- 3 = Devoluci칩n
    DATE(d.fecha) AS fechaMovimiento,
    dd.cantidad AS cantidad,            -- positivo: entrada de stock
    dd.precio AS costoSinIva,
    dd.total AS costoConIva
FROM devoluciones d
JOIN devoluciones_detalles dd ON d.id = dd.devolucion
WHERE DATE(d.fecha) BETWEEN STR_TO_DATE('${FECHA_SIGUIENTE}', '%Y-%m-%d')
                        AND STR_TO_DATE('${FECHA_ACTUAL}', '%Y-%m-%d')

UNION ALL

-- 游린 ANULACIONES
SELECT 
    a.id AS idMovimiento,
    ad.producto AS productoKey,
    a.sucursal AS sucursalKey,
    4 AS tipoMovimientoKey,             -- 4 = Anulaci칩n
    DATE(a.fecha) AS fechaMovimiento,
    ad.cantidad * -1 AS cantidad,       -- negativo: salida de stock
    ad.precio AS costoSinIva,
    ad.total AS costoConIva
FROM anulaciones a
JOIN anulaciones_detalles ad ON a.id = ad.anulacion
WHERE DATE(a.fecha) BETWEEN STR_TO_DATE('${FECHA_SIGUIENTE}', '%Y-%m-%d')
                        AND STR_TO_DATE('${FECHA_ACTUAL}', '%Y-%m-%d');
</sql>
