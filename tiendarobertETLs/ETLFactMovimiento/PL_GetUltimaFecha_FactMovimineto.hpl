    <limit>0</limit>
    <sql>-- devuelve la fila de dimtiempo asociada a la Ãºltima fecha real referenciada por factmovimiento
SELECT t.tiempoKey,
       DATE(t.fecha) AS ultima_fecha
FROM factmovimiento f
JOIN dimtiempo t ON f.fechaMovimientoKey = t.tiempoKey
ORDER BY t.fecha DESC
LIMIT 1;
</sql>
