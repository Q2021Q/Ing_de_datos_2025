    <sql>-- ==============================================
--  PL_CargaIncremental_FactMovimiento.hpl
--  Autor: CÃ©sar
--  DescripciÃ³n: Carga incremental de movimientos
-- ==============================================

-- ðŸ”¹ 1. Ventas
SELECT 
    v.id AS idMovimiento,
    vd.producto AS productoKey,
    v.sucursal AS sucursalKey,
    1 AS tipoMovimientoKey,           -- 1 = Venta
    DATE(v.fecha) AS fechaMovimiento,
    vd.cantidad * -1 AS cantidad,     -- negativa: salida de stock
    vd.costo AS costoSinIva,
    vd.total AS costoConIva
FROM ventas v
JOIN ventas_detalles vd ON v.id = vd.venta
WHERE DATE(v.fecha) > COALESCE(STR_TO_DATE('${ULTIMA_FECHA}', '%Y-%m-%d'), '1900-01-01')

UNION ALL

-- ðŸ”¹ 2. Compras
SELECT 
    c.id AS idMovimiento,
    cd.producto AS productoKey,
    c.sucursal AS sucursalKey,
    2 AS tipoMovimientoKey,           -- 2 = Compra
    DATE(c.fecha) AS fechaMovimiento,
    cd.cantidad AS cantidad,          -- positiva: entra stock
    cd.costo AS costoSinIva,
    cd.total AS costoConIva
FROM compras c
JOIN compras_detalles cd ON c.id = cd.compra
WHERE DATE(c.fecha) > COALESCE(STR_TO_DATE('${ULTIMA_FECHA}', '%Y-%m-%d'), '1900-01-01')

UNION ALL

-- ðŸ”¹ 3. Devoluciones
SELECT 
    d.id AS idMovimiento,
    dd.producto AS productoKey,
    d.sucursal AS sucursalKey,
    3 AS tipoMovimientoKey,           -- 3 = DevoluciÃ³n
    DATE(d.fecha) AS fechaMovimiento,
    dd.cantidad AS cantidad,
    dd.precio AS costoSinIva,         -- precio sin IVA
    dd.total AS costoConIva
FROM devoluciones d
JOIN devoluciones_detalles dd ON d.id = dd.devolucion
WHERE DATE(d.fecha) > COALESCE(STR_TO_DATE('${ULTIMA_FECHA}', '%Y-%m-%d'), '1900-01-01')

UNION ALL

-- ðŸ”¹ 4. Ajustes
SELECT 
    a.id AS idMovimiento,
    ad.producto AS productoKey,
    a.sucursal AS sucursalKey,
    4 AS tipoMovimientoKey,           -- 4 = Ajuste
    DATE(a.fecha) AS fechaMovimiento,
    ad.cantidad AS cantidad,
    ad.costo AS costoSinIva,
    ad.total AS costoConIva
FROM ajustes a
JOIN ajustes_detalles ad ON a.id = ad.ajuste
WHERE DATE(a.fecha) > COALESCE(STR_TO_DATE('${ULTIMA_FECHA}', '%Y-%m-%d'), '1900-01-01');
</sql>
