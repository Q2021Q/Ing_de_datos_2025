  <transform>
    <name>Get ultima fecha del DWH </name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>DWH_MYSQL</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT
  /* última fecha ya cargada */
  (SELECT MAX(fechaMovimientoKey) FROM factMovimiento) AS ultima_fecha_inventario,


 /* Días después de la última fecha cargada */
  (SELECT
    CAST(
      DATE_FORMAT(
        DATE_ADD(STR_TO_DATE(MAX(fechaMovimientoKey), '%Y%m%d'), INTERVAL 1 DAY),
        '%Y%m%d'
      ) AS UNSIGNED
    )
  FROM factMovimiento
) AS fecha_actual;


/* Fecha del día actual 
  (SELECT MAX(tiempoKey)
  FROM dimtiempo
  WHERE fecha &lt;= CURDATE()) AS fecha_actual; */
</sql>
